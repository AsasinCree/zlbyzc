<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="zlbyzc.sub3.duozhunze.Descition"
		table="tbdescition">
		<id name="id" >
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="varName" length="64" not-null="true" />
		</property>
		<property name="Consistency" type="java.lang.String">
			<column name="varConsistency" length="64" not-null="true" />
		</property>
		<property name="VarScheme" type="java.lang.String">
			<column name="varScheme"  not-null="false" />
		</property>
		<property name="varLastConsistency" type="java.lang.String">
			<column name="varLastConsistency"  not-null="false" />
		</property>
		<property name="date" type="java.util.Date">
			<column name="time"  />
		</property>
		
	</class>
</hibernate-mapping>
