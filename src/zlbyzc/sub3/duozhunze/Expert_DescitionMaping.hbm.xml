<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="zlbyzc.sub3.duozhunze.Expert_Descition"
		table="tbexpert_descition">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		
		<property name="expert_id" type="java.lang.Integer">
			<column name="intExpert" length="16" not-null="true" />
		</property>
		
		<property name="descition_id" type="java.lang.Integer">
			<column name="intDescition" length="16" not-null="true" />
		</property>
		
	</class>
</hibernate-mapping>
