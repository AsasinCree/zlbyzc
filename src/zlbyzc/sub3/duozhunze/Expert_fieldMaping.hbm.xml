<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="zlbyzc.sub3.duozhunze.Expert_field"
		table="tbexpert_field">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		
		<property name="expert_id" type="java.lang.Integer">
			<column name="intExpertId" length="11" not-null="true" />
		</property>
		
		<property name="field_id" type="java.lang.Integer">
			<column name="intField" length="11" not-null="true" />
		</property>
		
	</class>
</hibernate-mapping>
