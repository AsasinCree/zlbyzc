<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="zlbyzc.sub3.duozhunze.Scheme"
		table="tbscheme">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		
		<property name="name" type="java.lang.String">
			<column name="varName" length="60" not-null="true" />
		</property>
		
		<property name="descripition" type="java.lang.String">
			<column name="varDescripition" length="255" not-null="false" />
		</property>
		
		
		<property name="score" type="java.lang.String">
			<column name="varScore" length="16" not-null="false" />
		</property>
		
		<property name="descition_id" type="java.lang.Integer">
			<column name="intDescitionId" length="11" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
