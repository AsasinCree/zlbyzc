<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="zlbyzc.sub3.duozhunze.Field"
		table="tbfield">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		
		<property name="name" type="java.lang.String">
			<column name="varName" length="60" not-null="true" />
		</property>
		
		<property name="descipition" type="java.lang.String">
			<column name="varDescipition" length="512" not-null="true" />
		</property>
		
		
		<property name="Father_id" type="java.lang.Integer">
			<column name="varFatherFieldId" length="11" not-null="false" />
		</property>
	</class>
</hibernate-mapping>
