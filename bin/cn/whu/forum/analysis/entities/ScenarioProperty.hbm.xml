<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.whu.forum.analysis.entities.ScenarioProperty" table="sas_scenario_property">
		<id name="propertyID" column="property_id" type="integer">
			<generator class="native"></generator>
		</id>
		<many-to-one name="scenarioTask" class="cn.whu.forum.analysis.entities.ScenarioTask" outer-join="true">
			<column name="task_id"></column>
		</many-to-one>
		<property name="propertyType" type="string" column="property_type" length="200" />
		<property name="propertyContent" type="text" column="property_content" />
		<property name="mark1" type="string" column="mark1" length="200" />
		<property name="mark2" type="string" column="mark2" length="200" />
	</class>
</hibernate-mapping>
